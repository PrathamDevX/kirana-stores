{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "development",
            "cwd": "src",
            "program": "lib/main/main_development.dart",
            "request": "launch",
            "type": "dart",
            "args": [
          "--web-port",
          "63166",
          "--target",
          "lib/main/main_production.dart",

          "--web-experimental-hot-reload",
        ]
        },
            {
            "name": "production",
            "cwd": "src",
            "program": "lib/main/main_production.dart",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "src (profile mode)",
            "cwd": "src",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "src (release mode)",
            "cwd": "src",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}